{
    // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
    // 기존 특성에 대한 설명을 보려면 가리킵니다.
    // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm start",
            "name": "디버그 모드로 시작",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "프로세스에 연결",
            "processId": "${command:PickProcess}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "프로그램 시작(사용금지)",
            "skipFiles": [
                "<node_internals>/**"
            ],
          //  "program": "${workspaceFolder}\\index.js"
            "program": "${workspaceFolder}/app.js" , "env": { "NODE_ENV": "development" }
        }
    ]
}